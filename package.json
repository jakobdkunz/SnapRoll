{
  "name": "snaproll",
  "private": true,
  "packageManager": "pnpm@9.7.0",
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "convex:dev": "npx convex dev",
    "convex:deploy": "npx convex deploy",
    "convex:codegen": "npx convex codegen",
    "build:teacher-web": "turbo run build --filter=@snaproll/teacher-web",
    "build:student-web": "turbo run build --filter=@snaproll/student-web"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/papaparse": "5.3.14",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "turbo": "^2.0.6",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.6",
    "@vercel/blob": "^0.22.0",
    "convex": "^1.26.1",
    "papaparse": "5.4.1",
    "pdfjs-dist": "^3.11.174",
    "react-icons": "4.12.0",
    "zod": "^3.23.8"
  }
}
