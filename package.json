{
  "name": "snaproll",
  "private": true,
  "packageManager": "pnpm@9.7.0",
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "build:api": "turbo run build --filter=@snaproll/api",
    "build:teacher-web": "turbo run build --filter=@snaproll/teacher-web",
    "build:student-web": "turbo run build --filter=@snaproll/student-web"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "tsx": "^4.19.1",
    "turbo": "^2.0.6",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "react-icons": "4.12.0",
    "zod": "^3.23.8"
  }
}
